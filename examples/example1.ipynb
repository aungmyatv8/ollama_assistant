      "def binary_search(arr, target):\n",
      "    left, right = 0, len(arr) - 1\n",
      "    \n",
      "    while left <= right:\n",
      "        mid = (left + right) // 2\n",
      "        \n",
      "        if arr[mid] == target:\n",
      "            return mid\n",
      "        elif arr[mid] < target:\n",
      "            left = mid + 1\n",
      "        else:\n",
      "            right = mid - 1\n",
      "    \n",
      "    return -1\n",
      "```"
